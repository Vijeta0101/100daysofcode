#include <stdio.h>
#include <string.h>

int main() {
    char str1[] = "Hello";
    char str2[] = "World";

    // Swap the two strings using a temporary variable
    char temp[50];
    strcpy(temp, str1);
    strcpy(str1, str2);
    strcpy(str2, temp);

    // Print the swapped strings
    printf("str1: %s\n", str1);
    printf("str2: %s\n", str2);

    return 0;
}
